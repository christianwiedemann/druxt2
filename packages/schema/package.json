{
  "name": "@druxt2/schema",
  "version": "0.0.2",
  "description": "The Fully Decoupled Drupal Framework for Nuxt.js.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "module",
    "vue"
  ],
  "homepage": "https://github.com/christianwiedemann/druxt2",
  "bugs": {
    "url": "https://github.com/christianwiedemann/druxt2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxt-module-build && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepack": "nuxt-module-build"
  },
  "dependencies": {
    "@druxt2/druxt": "0.0.2"
  },
  "peerDependencies": {
    "consola": "*"
  },
  "optionalDependencies": {
    "core-js": "^3.16.1",
    "vue": "^3.2.26"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/christianwiedemann/druxt2.git"
}
