{
  "name": "@druxt2/views",
  "version": "0.0.2",
  "description": "The Fully Decoupled Drupal Framework for Nuxt.js.",
  "keywords": [
    "cms",
    "decoupled",
    "drupal",
    "druxt",
    "headless",
    "jsonapi",
    "json:api",
    "nuxt",
    "module",
    "vue"
  ],
  "homepage": "https://gitlab.devel.techcast.com/devel/eep",
  "bugs": {
    "url": "https://gitlab.devel.techcast.com/devel/eep"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/druxt/druxt.js"
  },
  "license": "MIT",
